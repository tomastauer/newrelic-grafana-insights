{
  "name": "newrelic-insights-grafana",
  "version": "1.0.0",
  "description": "Simple plugin for quering new relic using NRQL",
  "scripts": {
    "build": "grafana-toolkit plugin:build && cp -R dist /usr/local/var/lib/grafana/plugins",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev && cp -R dist /usr/local/var/lib/grafana/plugins",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "author": "Tomas Tauer",
  "license": "MIT",
  "devDependencies": {
    "@grafana/data": "7.0.1",
    "@grafana/toolkit": "7.0.1",
    "@grafana/ui": "7.0.1",
    "@testing-library/jest-dom": "5.4.0",
    "@testing-library/react": "^10.0.2",
    "@types/lodash": "latest"
  },
  "engines": {
    "node": ">=12 <13"
  },
  "dependencies": {
    "@grafana/runtime": "^7.1.5",
    "lodash": "^4.17.15",
    "react": "^16.13.1"
  }
}
